                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{Binding PlaceholderForeground, RelativeSource={RelativeSource Mode=TemplatedParent}, TargetNullValue={ThemeResource ComboBoxForegroundDisabled}}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropDownGlyph" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource ComboBoxDropDownGlyphForegroundDisabled}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="FocusStates">
                                    <VisualState x:Name="Focused">
                                        <Storyboard>
                                            <DoubleAnimation Duration="0" Storyboard.TargetName="HighlightBackground" Storyboard.TargetProperty="Opacity" To="1"/>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HighlightBackground" Storyboard.TargetProperty="BorderBrush">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource ComboBoxBackgroundBorderBrushFocused}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentPresenter" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource ComboBoxForegroundFocused}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PlaceholderTextBlock" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{Binding PlaceholderForeground, RelativeSource={RelativeSource Mode=TemplatedParent}, TargetNullValue={ThemeResource ComboBoxForegroundFocused}}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropDownGlyph" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource ComboBoxDropDownGlyphForegroundFocused}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="FocusedPressed">
                                        <Storyboard>
                                            <DoubleAnimation Duration="0" Storyboard.TargetName="HighlightBackground" Storyboard.TargetProperty="Opacity" To="1"/>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentPresenter" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource ComboBoxForegroundFocusedPressed}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PlaceholderTextBlock" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{Binding PlaceholderForeground, RelativeSource={RelativeSource Mode=TemplatedParent}, TargetNullValue={ThemeResource ComboBoxPlaceHolderForegroundFocusedPressed}}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropDownGlyph" Storyboard.TargetProperty="Foreground">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource ComboBoxDropDownGlyphForegroundFocusedPressed}"/>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Unfocused"/>
                                    <VisualState x:Name="PointerFocused"/>
                                    <VisualState x:Name="FocusedDropDown">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="PopupBorder" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0">
                                                    <DiscreteObjectKeyFrame.Value>
                                                        <Visibility>Visible</Visibility>
                                                    </DiscreteObjectKeyFrame.Value>
                                                </DiscreteObjectKeyFrame>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="DropDownStates">
                                    <VisualState x:Name="Opened">
                                        <Storyboard>
                                            <SplitOpenThemeAnimation ClosedTargetName="ContentPresenter" OpenedLength="{Binding TemplateSettings.DropDownOpenedHeight, RelativeSource={RelativeSource Mode=TemplatedParent}}" OffsetFromCenter="{Binding TemplateSettings.DropDownOffset, RelativeSource={RelativeSource Mode=TemplatedParent}}" OpenedTargetName="PopupBorder"/>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Closed">
                                        <Storyboard>
                                            <SplitCloseThemeAnimation ClosedTargetName="ContentPresenter" OpenedLength="{Binding TemplateSettings.DropDownOpenedHeight, RelativeSource={RelativeSource Mode=TemplatedParent}}" OffsetFromCenter="{Binding TemplateSettings.DropDownOffset, RelativeSource={RelativeSource Mode=TemplatedParent}}" OpenedTargetName="PopupBorder"/>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <ContentPresenter x:Name="HeaderContentPresenter" ContentTemplate="{TemplateBinding HeaderTemplate}" Content="{TemplateBinding Header}" FlowDirection="{TemplateBinding FlowDirection}" FontWeight="{ThemeResource ComboBoxHeaderThemeFontWeight}" Margin="{ThemeResource ComboBoxHeaderThemeMargin}" Visibility="Collapsed" x:DeferLoadStrategy="Lazy"/>
                            <Border x:Name="Background" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Grid.ColumnSpan="2" Grid.Row="1"/>
                            <Border x:Name="HighlightBackground" Background="{ThemeResource ComboBoxBackgroundUnfocused}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{ThemeResource ComboBoxBackgroundBorderBrushUnfocused}" Grid.ColumnSpan="2" Opacity="0" Grid.Row="1"/>
                            <ContentPresenter x:Name="ContentPresenter" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" Grid.Row="1" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                <TextBlock x:Name="PlaceholderTextBlock" Foreground="{Binding PlaceholderForeground, RelativeSource={RelativeSource Mode=TemplatedParent}, TargetNullValue={ThemeResource ComboBoxPlaceHolderForeground}}" Text="{TemplateBinding PlaceholderText}"/>
                            </ContentPresenter>
                            <FontIcon x:Name="DropDownGlyph" AutomationProperties.AccessibilityView="Raw" Grid.Column="1" FontFamily="{ThemeResource SymbolThemeFontFamily}" Foreground="White" FontSize="12" Glyph="&#xE0E5;" HorizontalAlignment="Right" IsHitTestVisible="False" Margin="0,10,10,10" Grid.Row="1" VerticalAlignment="Center"/>
                            <Popup x:Name="Popup">
                                <Border x:Name="PopupBorder" Background="{ThemeResource ComboBoxDropDownBackground}" BorderThickness="{ThemeResource ComboBoxDropdownBorderThickness}" BorderBrush="{ThemeResource ComboBoxDropDownBorderBrush}" HorizontalAlignment="Stretch" Margin="0,-1,0,-1">
                                    <ScrollViewer x:Name="ScrollViewer" AutomationProperties.AccessibilityView="Raw" BringIntoViewOnFocusChange="{TemplateBinding ScrollViewer.BringIntoViewOnFocusChange}" Foreground="{ThemeResource ComboBoxDropDownForeground}" HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}" HorizontalScrollMode="{TemplateBinding ScrollViewer.HorizontalScrollMode}" IsDeferredScrollingEnabled="{TemplateBinding ScrollViewer.IsDeferredScrollingEnabled}" IsHorizontalRailEnabled="{TemplateBinding ScrollViewer.IsHorizontalRailEnabled}" IsVerticalRailEnabled="{TemplateBinding ScrollViewer.IsVerticalRailEnabled}" MinWidth="{Binding TemplateSettings.DropDownContentMinWidth, RelativeSource={RelativeSource Mode=TemplatedParent}}" VerticalSnapPointsType="OptionalSingle" VerticalScrollMode="{TemplateBinding ScrollViewer.VerticalScrollMode}" VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}" VerticalSnapPointsAlignment="Near" ZoomMode="Disabled">
                                        <ItemsPresenter Margin="{ThemeResource ComboBoxDropdownContentMargin}"/>
                                    </ScrollViewer>
                                </Border>
                            </Popup>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>

    <Grid>
        <Border x:Name="OutBorder"
                PointerReleased="OutBorder_PointerReleased"
                Background="#55000000" 
                VerticalAlignment="Stretch" 
                HorizontalAlignment="Stretch"/>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="3*" MinWidth="500"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="5*" MinHeight="325"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>

            <!---阴影-->
            <controls:DropShadowPanel BlurRadius="40"
                                      ShadowOpacity="1"
                                      Color="Black"
                                      Grid.ColumnSpan="1" Grid.Column="1" Grid.RowSpan="1" Grid.Row="1" Margin="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                <Rectangle RadiusX="30" RadiusY="30" Stroke="Black" StrokeThickness="2"/>
            </controls:DropShadowPanel>

            <Grid x:Name="backgroundGrid" Grid.Column="1"
                  Grid.Row="1"
                  CornerRadius="30"
                  Background="{ThemeResource SystemControlAccentDark1AcrylicWindowAccentDark1Brush}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="5*" MinWidth="500"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1.8*"/>
                        <RowDefinition Height="6*"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>
                    
                    <TextBox PlaceholderText="事件标题" Style="{StaticResource NameTextBoxStyle}" Grid.Column="1"/>

                    <ListView Grid.Column="1"
                              Grid.Row="1"
                              Margin="0,0,0,21"
                              SelectionMode="None"
                              IsItemClickEnabled="False">
                        <ListViewItem HorizontalContentAlignment="Stretch"
                                      VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon Symbol="Clock" Foreground="White"/>
                                <TextBlock Grid.Column="1" Text="全天事件" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="White"/>
                                <ToggleSwitch x:Name="allDayEventToggleSwitch" Toggled="ToggleSwitch_Toggled" Style="{StaticResource ToggleSwitchStyle}" Margin="10,0,10,0" Grid.Column="1" MinWidth="80" Foreground="White" FocusVisualPrimaryBrush="White" HorizontalAlignment="Right"/>
                            </Grid>
                        </ListViewItem>
                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="开始时间" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <DatePicker x:Name="startTimeDatePicker" Grid.Column="2" Style="{StaticResource DatePickerStyle}" MinWidth="150" Margin="10,0,10,0" HorizontalAlignment="Stretch" BorderThickness="1" Foreground="White" Background="Transparent"/>
                                <TimePicker x:Name="startTimeTimePicker" Style="{StaticResource TimePickerStyle}" Grid.Column="3" MinWidth="80" HorizontalAlignment="Stretch" Margin="10,0,10,0" BorderThickness="1"/>
                            </Grid>
                        </ListViewItem>
                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="结束时间" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <DatePicker x:Name="endTimeDatePicker" Grid.Column="2" Style="{StaticResource DatePickerStyle}" MinWidth="150" Margin="10,0,10,0" HorizontalAlignment="Stretch" BorderThickness="1" Foreground="White" Background="Transparent"/>
                                <TimePicker x:Name="endTimeTimePicker" Style="{StaticResource TimePickerStyle}" Grid.Column="3" MinWidth="80" HorizontalAlignment="Stretch" Margin="10,0,10,0" BorderThickness="1"/>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon Symbol="Refresh" Foreground="White"/>
                                <TextBlock Grid.Column="1" Text="重复" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <ToggleSwitch x:Name="repeatToggleSwitch" Toggled="ToggleSwitch_Toggled" Style="{StaticResource ToggleSwitchStyle}" Margin="10,0,10,0" Grid.Column="2" MinWidth="80" Foreground="White" FocusVisualPrimaryBrush="White" HorizontalAlignment="Right"/>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem x:Name="repeatTypeListViewItem" Visibility="Collapsed" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="重复形式" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <ComboBox x:Name="repeatTypeComboBox" Style="{StaticResource ComboBoxStyle}" SelectionChanged="RepeatTypeComboBox_SelectionChanged" Grid.Column="2" BorderThickness="0" Margin="10,0" Background="Transparent" HorizontalContentAlignment="Right" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Stretch">
                                    <x:String>按天重复</x:String>
                                    <x:String>按周重复</x:String>
                                    <x:String>按月重复</x:String>
                                    <x:String>按年重复</x:String>
                                </ComboBox>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem x:Name="repeatCycleListViewItem" Visibility="Collapsed" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="重复周期" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <TextBlock Grid.Column="3" Text="每" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="White" Margin="15,0"/>
                                <TextBox x:Name="repeatCycleCountTextBlock" Style="{StaticResource TextBoxStyle}" Text="1" Grid.Column="3" Margin="5,0,0,0" MinWidth="15" HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                                <TextBlock x:Name="repeatCycleUnitTextBlock" Grid.Column="3" Text="天" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="White" Margin="15,0"/>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem x:Name="repeatUntilListViewItem" Visibility="Collapsed" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="重复直到" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <DatePicker x:Name="repeatEndTimeDatePicker" Grid.Column="2" Style="{StaticResource DatePickerStyle}" MinWidth="150" Margin="10,0,10,0" HorizontalAlignment="Stretch" BorderThickness="1" Foreground="White" Background="Transparent"/>
                                <TimePicker x:Name="repeatEndTimeTimePicker" IsEnabled="False" Style="{StaticResource TimePickerStyle}" Grid.Column="3" MinWidth="80" HorizontalAlignment="Stretch" Margin="10,0,10,0" BorderThickness="1"/>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon Symbol="Map" Foreground="White"/>
                                <TextBox Style="{StaticResource TextBoxStyle}" Grid.Column="1" PlaceholderText="编辑发生地点" Margin="10,0,10,0" VerticalAlignment="Center" Foreground="White"/>

                            </Grid>
                        </ListViewItem>

                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon Symbol="Tag" Foreground="White"/>
                                <TextBlock Grid.Column="1" Text="事件类型" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <ComboBox Style="{StaticResource ComboBoxStyle}" SelectedValue="一般事件" Grid.Column="2" BorderThickness="0" Margin="10,0" Background="Transparent" HorizontalContentAlignment="Right" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Stretch">
                                    <x:String>一般事件</x:String>
                                    <x:String>时间轴事件</x:String>
                                </ComboBox>
                            </Grid>
                        </ListViewItem>

                        

                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon Symbol="Bullets" Foreground="White"/>
                                <TextBox Style="{StaticResource TextBoxStyle}" Grid.Column="1" PlaceholderText="编辑备注" Margin="10,0,10,0" VerticalAlignment="Center" Foreground="White"/>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBox Style="{StaticResource TextBoxStyle}" Grid.Column="1" PlaceholderText="设置状态显示信息" Margin="10,0,10,0" VerticalAlignment="Center" Foreground="White"/>
                            </Grid>
                        </ListViewItem>

                        <ListViewItem HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="1.5*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon x:Name="colorChoiceIcon" Symbol="Highlight" Foreground="White"/>
                                <TextBlock Grid.Column="1" Text="颜色" Margin="10" VerticalAlignment="Center" Foreground="White"/>
                                <Rectangle x:Name="colorChoiceShowRectangle" RadiusX="10" RadiusY="10" Tapped="ColorRectangle_Tapped" Grid.Column="2" Width="100" Height="20" HorizontalAlignment="Right" Margin="20,0" Fill="Black">
                                    <FlyoutBase.AttachedFlyout>
                                        <Flyout>
                                            <Flyout.FlyoutPresenterStyle>
                                                <Style TargetType="FlyoutPresenter">
                                                    <Setter Property="Background" Value="{ThemeResource SystemControlAccentDark1AcrylicWindowAccentDark1Brush}"/>
                                                    <Setter Property="ScrollViewer.HorizontalScrollMode" Value="Disabled"/>
                                                    <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled"/>
                                                    <Setter Property="IsTabStop" Value="True"/>
                                                    <Setter Property="TabNavigation" Value="Cycle"/>
                                                </Style>
                                            </Flyout.FlyoutPresenterStyle>
                                            <ColorPicker x:Name="colorChoiceColorPicker" Foreground="Transparent" ColorSpectrumShape="Ring" IsHexInputVisible="False" IsColorPreviewVisible="False" IsColorChannelTextInputVisible="False" ColorChanged="ColorPicker_ColorChanged" Grid.Column="2" Color="WhiteSmoke" HorizontalAlignment="Stretch" Margin="10"/>
                                        </Flyout>
                                    </FlyoutBase.AttachedFlyout>
                                </Rectangle>
                            </Grid>
                        </ListViewItem>


                        <ListViewItem HorizontalContentAlignment="Stretch"
                                      VerticalContentAlignment="Stretch">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <SymbolIcon Symbol="BlockContact" Foreground="White"/>
                                <TextBlock Grid.Column="1" Text="事件私有" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="White"/>
                                <ToggleSwitch Style="{StaticResource ToggleSwitchStyle}" Margin="10,0,10,0" Grid.Column="1" MinWidth="80" Foreground="White" FocusVisualPrimaryBrush="White" HorizontalAlignment="Right"/>
                            </Grid>
                        </ListViewItem>

                    </ListView>
                    
                    <Button Style="{StaticResource ButtonRevealStyle}" BorderThickness="1" Content="确认" Width="106" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Top" Foreground="White"/>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
